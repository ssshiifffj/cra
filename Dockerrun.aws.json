{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
       {
          "name":"client",
          "image":"mo29172/client",
          "hostname":"client",
          "essential":false
       },
       {
          "name":"server",
          "image":"mo29172/server",
          "hostname":"api",
          "essential":false
       },
       {
          "name":"worker",
          "image":"mo29172/worker",
          "essential":false
       },
       {
          "name": "postgres",
          "image": "postgres",
          "hostname": "postgres",
          "essential": false,
          "environment": [
             {
                "name": "POSTGRES_USER",
                "value": "user"
             },
             {
                "name": "POSTGRES_PASSWORD",
                "value": "user"
             },
             {
                "name": "POSTGRES_DB",
                "value": "db"
             }
          ]
       },
       {
          "name": "redis",
          "image": "redis",
          "hostname": "redis",
          "essential": false
       },
       {
          "name":"nginx",
          "image":"mo29172/nginx",
          "essential":true,
          "portMappings":[
             {
                "hostPort":80,
                "containerPort":80
             }
          ],
          "links":[
             "client",
             "server"
          ]
       }
    ]
 }
